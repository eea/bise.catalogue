- content_for :pageheader do
  Unprocessed Objects


%table.table.table-striped
  %thead
    %tr
      %th Model
      %th Method
      %th Failed Field
      %th Message
      %th IP
      %th Raised
  %tbody
    - @unprocessed.each do |u|
      %tr
        %td= u.model
        %td= u.http_method
        %td= u.field
        %td= u.message
        %td= u.ip
        %td
          = link_to biseadmin_unprocessed_object_path(u) do
            = u.created_at.try(:strftime,'%Y/%m/%d')

= will_paginate @unprocessed, renderer: BootstrapPagination::Rails

