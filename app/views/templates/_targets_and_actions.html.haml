.row
  .col-md-6
    %legend Biodiversity Strategy Targets
    .row
      .col-lg-12
        - if object.targets.size > 0
          - object.targets.each do |t|
            .label.label-success= t.name
        - else
          No targets associated.
  .col-md-6
    %legend Biodiversity Strategy Actions
    .row
      .col-lg-12
        - if object.actions.size > 0
          - object.actions.each do |t|
            .label.label-success= t.name
        - else
          No targets associated.
%br